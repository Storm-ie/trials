<?xml version="1.0"?>
<pnp:Provisioning xmlns:pnp="http://schemas.dev.office.com/PnP/2020/02/ProvisioningSchema">
  <pnp:Templates>
    <pnp:ProvisioningTemplate ID="Provisioning.Main.Teams.Navigation">
      <pnp:Navigation AddNewPagesToNavigation="false" CreateFriendlyUrlsForNewPages="false">
        <pnp:GlobalNavigation NavigationType="Structural">
          <pnp:StructuralNavigation RemoveExistingNodes="true" />
        </pnp:GlobalNavigation>
        <pnp:CurrentNavigation NavigationType="StructuralLocal">
          <pnp:StructuralNavigation RemoveExistingNodes="true">
            <pnp:NavigationNode Title="Home" Url="{site}" />
            <pnp:NavigationNode Title="Provisioning Requests" Url="{site}/{listurl:Provisioning Requests}" />
            <pnp:NavigationNode Title="Provisioning Templates" Url="{site}/{listurl:Provisioning Templates}" />
            <pnp:NavigationNode Title="Provisioning Custom Properties" Url="{site}/{listurl:Provisioning Custom Properties}" />
          </pnp:StructuralNavigation>
        </pnp:CurrentNavigation>
      </pnp:Navigation>
    </pnp:ProvisioningTemplate>
  </pnp:Templates>
</pnp:Provisioning>